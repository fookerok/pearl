{
  "rewrites": [
    {
      "source": "/jemchujina",
      "destination": "/index.html"
    },
    {
      "source": "/jemchujina/:path((?!.*\\.).*)",
      "destination": "/:path.html"
    }
  ],
  "redirects": [
    { "source": "/jemchujina", "destination": "/", "permanent": true }
  ]
}